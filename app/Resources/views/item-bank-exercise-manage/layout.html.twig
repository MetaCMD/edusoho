{% extends 'v2/layout.html.twig' %}

{% set active_tab = 'exercise-set' %}
{% block full_content %}
  {% block top_content %}
    <div class="{% block sectionClass %} courseset-header-section {% endblock %}">
      <div class="container cd-container">
        {% block breadcrumb %}
        {% endblock %}
        {% block courseset_header %}
        {% endblock %}
        {% block course_header %}
        {% endblock %}
      </div>
    </div>
  {% endblock %}
  <div class="container cd-container">
    <div class="cd-content courseset-manage-body {% block paddingClass %} courseset-manage-padding {% endblock %}">
      {{ render(controller('AppBundle:My/User:avatarAlert')) }}
      {% block sidebar %}
      {% endblock %}

      {% block main %}
      {% endblock %}
    </div>
  </div>
{% endblock %}
